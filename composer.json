{
  "name": "superstani/telegram_bot",
  "type": "library",
  "description": "my personal framework for telegram bot developments",
  "keywords": [
    "telegram",
    "bot",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/SuperStani/telegram_bot",
  "authors": [
    {
      "name": "Stanislav",
      "email": "stanislavzatic.dev@gmail.com",
      "role": "Creator"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 3600
  },
  "require": {
    "ext-json": "*",
    "ext-pdo": "*",
    "guzzlehttp/guzzle": "^7.0",
    "intervention/image": "^2.7",
    "php-di/php-di": "^6.4"
  }
}
